class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int ans=0;
        int n=prices.size();
        //ham har din buy and sell kar sakte hai to uska fyada uthyenge har din check kar ke add kar denge profit;
        for(int i=1;i<n;i++){
if(prices[i]>prices[i-1]){
    ans+=prices[i]-prices[i-1];
}
        }
        return ans;
    }
};

#include <bits/stdc++.h>
using namespace std;

void print(int i,int arr[],int n){
  if(i>=n){
    return ;
  }
  //1 case solve kardiya i.e first element print kar diya 
  cout<<arr[i]<<" ";

  //then recurssive call kar diya
  print(i+1,arr,n);
}


int main() {
  
  int i=0;
  int n=5;

 int arr[5]={2,3,4,5,6};
print(i,arr,n);
  return 0;
}

#include<bits/stdc++.h>
using namespace std;

class graph{
   public: unordered_map<int,list<int>>adjlist;

   void addedge(int u,int v,bool  direction){
adjlist[u].push_back(v);
if(direction==0){
   adjlist[v].push_back(u);
}
   }
void printlist(){
   for(auto node:adjlist){
      cout<<node.first<<"-> ";
      for(auto value:node.second){
cout<<value<<" ";
      }
      cout<<endl;
   }
}
};
int main(){
 
graph g;

 g.addedge(0,1,0);
 g.addedge(1,2,0);
 g.addedge(0,2,0);
   
   g.printlist();
   return 0;
}
